{
  "name": "monorepo-boilerplate",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn install; lerna bootstrap;",
    "start": "lerna run start --parallel",
    "start:client": "lerna run --scope client --stream dev",
    "build:client": "lerna run --scope client --stream build",
    "run:build:client": "lerna run start --scope client"
  },
  "workspaces": [
    "packages/*"
  ]
}
